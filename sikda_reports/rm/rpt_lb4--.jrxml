SELECT 'VII' AS 'GROUP', 'KEGIATAN PELAYANAN JPKM' AS 'NAMA_GROUP', 'Jumlah Kunjungan Peserta PT. Askes' AS 'URAIAN', 0 AS JUMLAH , 2 AS iRow
