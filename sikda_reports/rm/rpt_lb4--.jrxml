UNION ALL
SELECT 'IV' AS 'GROUP', 'FREKUENSI KEGIATAN PUSKESMAS DILUAR GEDUNG' AS 'NAMA_GROUP', 'Jumlah Frekuensi Kegiatan Imunisasi' AS 'URAIAN', 0 AS JUMLAH , 2 AS iRow
