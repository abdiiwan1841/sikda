COUNT(X.KD_PASIEN) as JML
FROM (SELECT * FROM (
select V.KD_PASIEN, V.KD_PELAYANAN, V.KD_PUSKESMAS, V.VARIABEL_ID AS 'CL_CHILDVITA', '' AS 'CL_CHILDEXAMAGE',  V.HASIL  AS 'VITA' , '' AS AGE from pel_hasil_kia AS V
WHERE V.KD_PUSKESMAS = $P{parameter1}
AND V.VARIABEL_ID = 'CL_CHILDVITA'
AND V.HASIL = 'YA'
UNION ALL
select V.KD_PASIEN, V.KD_PELAYANAN, V.KD_PUSKESMAS,  ''AS 'CL_CHILDVITA' ,V.VARIABEL_ID AS 'CL_CHILDEXAMAGE', '' AS 'VITA' , V.HASIL AS AGE from pel_hasil_kia AS V
WHERE V.KD_PUSKESMAS = $P{parameter1}
AND V.VARIABEL_ID = 'CL_CHILDEXAMAGE'
AND V.HASIL BETWEEN  '2190' AND '4015'
