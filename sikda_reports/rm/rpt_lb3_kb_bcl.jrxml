select  'KESEHATAN IBU DAN ANAK' as 'JUDUL',  'A' as 'KELOMPOK','Gizi' as Program, 'Jumlah anak balita (1-5 th) dapat kapsul vit. A dosis tinggi (200.000 IU) bulan ini' as kegiatan, IFNULL((SELECT SUM(ANAK_P_12_60_K_VIT_A)+SUM(ANAK_L_12_60_K_VIT_A) FROM t_ds_gizi WHERE BULAN=SELECT DATE_FORMAT($P{date1},'%m') AND TAHUN=SELECT DATE_FORMAT($P{date1},'%Y') AND kd_puskesmas=$P{parameter1}),0) as Jumlah6, 2 as iRow
